{
    "name": "asherkin/throttle",
    "type": "project",
    "description": "Lightweight google-breakpad back-end written in PHP and backed by MySQL",
    "homepage": "https://bitbucket.org/asherkin/throttle",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Asher Baker",
            "email": "asherkin@limetech.org",
            "homepage": "http://limetech.org"
        }
    ],
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "facebook/libphutil",
                "version": "dev-master",
                "source": {
                    "url": "https://github.com/facebook/libphutil.git",
                    "type": "git",
                    "reference": "origin/master"
                }
            }
        }
    ],
    "require": {
        "silex/silex": "~1.0.0@dev",
        "symfony/monolog-bridge": "~2.2.0",
        "monolog/monolog": "~1.0",
        "symfony/doctrine-bridge": "~2.2.0",
        "doctrine/dbal": "~2.2",
        "doctrine/migrations": "dev-master",
        "symfony/twig-bridge": "~2.2.0",
        "twig/twig": "~1.8",
        "cilex/console-service-provider": "~1.0.0@dev",
        "symfony/console": "~2.2.0",
        "silex/web-profiler": "~1.0.0@dev",
        "facebook/libphutil": "dev-master",
        "fp/lightopenid": "dev-master"
    },
    "autoload": {
        "psr-0": { "Throttle": "src/" }
    }
}
